{
  "expo": {
    "name": "BankTally",
    "slug": "banktally",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ic_launcher.png",
    "scheme": "banktally",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ztamoa.banktally"
    },
    "android": {
      "permissions": ["android.permission.BLUETOOTH_CONNECT"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/calc-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ztamoa.banktally"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "51fb757a-8e2a-4fb4-bdbf-749795c2aa7e"
      }
    }
  }
}
